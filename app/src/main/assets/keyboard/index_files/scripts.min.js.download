function remove_hover_in_css() {
    try {
        for (var b in document.styleSheets) {
            var d = document.styleSheets[b];
            if (!d.rules) {
                continue
            }
            for (var a = d.rules.length - 1; a >= 0; a--) {
                if (!d.rules[a].selectorText) {
                    continue
                }
                if (d.rules[a].selectorText.match(":hover")) {
                    d.deleteRule(a)
                }
            }
        }
    } catch (c) {}
}
