    var a = (g_input_text[g_cursor_pos - 1] == "?") ? g_cursor_pos - 1 : g_cursor_pos;
    g_input_text = g_input_text.substr(0, a) + b + g_input_text.substr(g_cursor_pos);
    g_cursor_pos = Number(a) + Number(b.length);
    if (b.length > 1) {
        var c = b.indexOf("?");
        if (c != -1) {
            g_cursor_pos = a + c + 1
        } else {
