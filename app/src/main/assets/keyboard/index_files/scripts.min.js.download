function app_version(c) {
    var b;
    var a = (c == "iOS") ? /Cymath\/iOS\/([0-9]+\.[0-9]+)/g : /Cymath\/Android\/([0-9]+\.[0-9]+)/g;
    var b = a.exec(navigator.userAgent);
    if (b) {
        if (b.length == 2) {
            return b[1]
        }
    }
    return ""
}

function get_back_q_from_local_storage() {
    if (window.localStorage) {
