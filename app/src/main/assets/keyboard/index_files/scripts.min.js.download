}

function fuzzy_search(g, f) {
    var e = f.length;
    var a = g.length;
    if (a > e) {
        return false
    }
    if (a === e) {
        return g === f
    }
    outer: for (var c = 0, b = 0; c < a; c++) {
        var d = g.charCodeAt(c);
        while (b < e) {
            if (f.charCodeAt(b++) === d) {
                continue outer
            }
        }
        return false
    }
    return true
}
